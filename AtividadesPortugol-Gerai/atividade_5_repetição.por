programa
{
	
	funcao inicio()
	{

	   inteiro n
	   inteiro x = 100

	   escreva("Informe um n√∫mero qualquer: ")
	   leia(n)

	   se(n < x){
	   	escreva(n)
	   	enquanto(n < x){
	   		escreva("\n", n = n + 1)	   
	   	}
	   	
	   }

	   
	   

	   se(n > x){
	   	escreva(n)
	   	enquanto( n > x){
	   		escreva("\n", n = n - 1)
	   	}
	   	
	   }

	 
	
		
	}
}
