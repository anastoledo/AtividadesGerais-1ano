programa
{
	
	funcao inicio()
	{

	   inteiro x
	   inteiro y

	   escreva("Informe um número qualquer: ")
	   leia(x)

	   escreva("Informe outro número: ")
	   leia(y)

	   se(y < x){
	   	enquanto(y < x){
	   		escreva("\n", y = y + 1)
	   		   
	   	}
	   	
	   }

	   se(y > x){
	   	enquanto( y > x){
	   		escreva("\n", y = y - 1 )
	   		
	   	}
	   	
	   }

	 
	
		
	}
}
